<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
    xmlns:Controls="using:App4di.Dotnet.ChronoView.WinUI.Control">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="App.AccentBrush" Color="#3A96DD"/>
            <SolidColorBrush x:Key="App.AccentBrush.Darker" Color="#2E7BB5"/>
            <SolidColorBrush x:Key="App.BackgroundBrush" Color="#1E1F22"/>
            <SolidColorBrush x:Key="App.SurfaceBrush" Color="#2A2B2F"/>

            <SolidColorBrush x:Key="App.SurfaceBorderBrush" Color="#3A3B40"/>
            <SolidColorBrush x:Key="App.OnSurfaceBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="App.SubtleTextBrush" Color="#B8B8B8"/>
            <SolidColorBrush x:Key="App.NavBackgroundBrush" Color="#141517"/>
            <SolidColorBrush x:Key="App.NavForegroundBrush" Color="#FFFFFF"/>

            <SolidColorBrush x:Key="Timeline.MarkerBrush" Color="#4DA8FF"/>
            <SolidColorBrush x:Key="Timeline.HoverBrush" Color="DarkGray"/>
            <SolidColorBrush x:Key="Timeline.SelectedBrush" Color="#FFFFFF"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
