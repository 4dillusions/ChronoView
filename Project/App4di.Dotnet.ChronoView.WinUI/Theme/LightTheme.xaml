<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
    xmlns:Controls="using:App4di.Dotnet.ChronoView.WinUI.Control">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="App.AccentBrush" Color="#0067C0"/>
            <SolidColorBrush x:Key="App.AccentBrush.Darker" Color="#0055A1"/>
            <SolidColorBrush x:Key="App.BackgroundBrush" Color="#FAFAFA"/>
            <SolidColorBrush x:Key="App.SurfaceBrush" Color="#FFFFFF"/>

            <SolidColorBrush x:Key="App.SurfaceBorderBrush" Color="#D9D9D9"/>
            <SolidColorBrush x:Key="App.OnSurfaceBrush" Color="#000000"/>
            <SolidColorBrush x:Key="App.SubtleTextBrush" Color="#6E6E6E"/>
            <SolidColorBrush x:Key="App.NavBackgroundBrush" Color="#EDEDED"/>
            <SolidColorBrush x:Key="App.NavForegroundBrush" Color="#1A1A1A"/>

            <SolidColorBrush x:Key="Timeline.MarkerBrush" Color="#0078D7"/>
            <SolidColorBrush x:Key="Timeline.HoverBrush" Color="DarkGray"/>
            <SolidColorBrush x:Key="Timeline.SelectedBrush" Color="#333333"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
